{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors"
  ],
  "rules": {
    "no-console": 1
  },
  "plugins": [
    "import"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
